{
  "name": "nest-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "prashant",
  "private": true,
  "license": "LICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:local": "cross-env NODE_ENV=local nest start --watch",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d ./type-orm.config.ts",
    "typeorm-generate:prod": "cross-env NODE_ENV=production npm run typeorm migration:generate ./src/database/migrations/ -n ",
    "typeorm-prod:migrate": "cross-env NODE_ENV=production npm run typeorm migration:run",
    "typeorm-generate:dev": "cross-env NODE_ENV=development npm run typeorm migration:generate ./src/database/migrations/ -n ",
    "typeorm-dev:migrate": "cross-env NODE_ENV=development npm run typeorm migration:run",
    "typeorm-dev:revert": "cross-env NODE_ENV=development npm run typeorm migration:revert",
    "typeorm-generate:local": "cross-env NODE_ENV=local npm run typeorm migration:generate ./src/database/migrations/$npm_config_name ",
    "typeorm-local:migrate": "cross-env NODE_ENV=local npm run typeorm migration:run",
    "typeorm-local:revert": "cross-env NODE_ENV=local npm run typeorm migration:revert",
    "mikro-generate:local": "cross-env NODE_ENV=local npx mikro-orm migration:create",
    "mikro-migrate:local": "cross-env NODE_ENV=local npx mikro-orm migration:up",
    "mikro-revert:local": "cross-env NODE_ENV=local npx mikro-orm migration:down",
    "mikro-list:local": "cross-env NODE_ENV=local npx mikro-orm migration:list",
    "mikro-pending:local": "cross-env NODE_ENV=local npx mikro-orm migration:pending",
    "mikro-generate:dev": "cross-env NODE_ENV=development npx mikro-orm migration:create",
    "mikro-migrate:dev": "cross-env NODE_ENV=development npx mikro-orm migration:up",
    "mikro-revert:dev": "cross-env NODE_ENV=development npx mikro-orm migration:down",
    "mikro-list:dev": "cross-env NODE_ENV=development npx mikro-orm migration:list",
    "mikro-pending:dev": "cross-env NODE_ENV=development npx mikro-orm migration:pending",
    "mikro-generate:prod": "cross-env NODE_ENV=production npx mikro-orm migration:create",
    "mikro-migrate:prod": "cross-env NODE_ENV=production npx mikro-orm migration:up",
    "mikro-revert:prod": "cross-env NODE_ENV=production npx mikro-orm migration:down",
    "mikro-list:prod": "cross-env NODE_ENV=production npx mikro-orm migration:list",
    "mikro-pending:prod": "cross-env NODE_ENV=production npx mikro-orm migration:pending"
  },
  "dependencies": {
    "@mikro-orm/cli": "^5.6.0",
    "@mikro-orm/core": "^5.6.0",
    "@mikro-orm/migrations": "^5.6.0",
    "@mikro-orm/nestjs": "^5.1.2",
    "@mikro-orm/postgresql": "^5.6.0",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/mongoose": "^9.2.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^6.1.3",
    "@nestjs/typeorm": "^9.0.1",
    "bcrypt": "^5.1.0",
    "cache-manager": "^5.1.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cross-env": "^7.0.3",
    "express-http-context": "^1.2.4",
    "handlebars": "^4.7.7",
    "mongodb": "^4.11.0",
    "mongoose": "^6.7.2",
    "morgan": "^1.10.0",
    "nest-winston": "^1.8.0",
    "nodemailer": "^6.8.0",
    "otp-generator": "^4.0.0",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.10",
    "uuid": "^9.0.0",
    "webpack": "^5.75.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.0",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.189",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.0.0",
    "@types/otp-generator": "^4.0.0",
    "@types/passport-facebook": "^2.1.11",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.7",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsdoc": "^39.3.14",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./mikro-orm.config"
    ]
  }
}
